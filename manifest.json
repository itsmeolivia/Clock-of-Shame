{
  "manifest_version": 2,

  "name": "Clock of Shame",
  "description": "You have wasted a lot of time on Facebook and u should be ashamed",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "tabs",
    "https://www.facebook.com/*"
  ],
  "content_scripts": [
  {
    "matches": ["https://www.facebook.com/*"],
    "css": [],
    "js": ["jquery.js", "main.js"]
  }
  ]
}
